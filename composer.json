{
    "name": "irealworlds/laravel-openapi",
    "type": "library",
    "autoload": {
        "psr-4": {
            "IrealWorlds\\OpenApi\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Marin Calin",
            "email": "marincalin2551@hotmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^10.17"
    },
    "autoload-dev": {
        "psr-4": {
            "IrealWorlds\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^8.5",
        "pestphp/pest": "^2.11"
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\IrealWorlds\\OpenApi\\Providers\\OpenApiServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
